{% set target = datas.target %}
{% set count = datas.count %}
<div class="contact_root">
    <div class="contact-head">
        <div class="target-name">{{ target.username }}</div>
        <div class="target-survive">{{ target.survive_days }}日生存</div>
    </div>
    <div class="contact-grid">
        <div>walk:{{ count.walk }}</div>
        <div>turn:{{ count.turn }}</div>
        <div>rocket:{{ count.rocket }}</div>
        <div>kill:{{ count.kill }}</div>
        <div>post:{{ count.post }}</div>
        <div>page:{{ count.page }}</div>
        <div>book:{{ count.book }}</div>
        <div>shelf:{{ count.shelf }}</div>
        <div>planet:{{ count.planet }}</div>
        <div>special:{{ count.special }}</div>
        <div>p.draw:{{ count.planet_draw }}</div>
        <div>u.draw:{{ count.user_draw }}</div>
    </div>
    <form class="contact-form" method="post" action="/">
        <button type="submit" name="action" value="forgive">見逃す</button>
        <button type="submit" name="action" value="kill">殺す</button>
    </form>
</div>
